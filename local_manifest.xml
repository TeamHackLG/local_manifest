<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<!-- Remote names -->
	<remote name="lineage" fetch="https://github.com/LineageOS/" />
	<remote name="thlg" fetch="https://github.com/TeamHackLG/" />
	<!-- Prebuilt apps -->
	<remove-project name="LineageOS/android_packages_apps_Eleven" /> <!-- Using BlackPlayer -->
	<remove-project name="LineageOS/android_packages_apps_Camera2" /> <!-- Using OpenCamera -->
	<!-- Adreno STLPort -->
	<project path="external/stlport" name="android_external_stlport" revision="cm-14.1" remote="lineage" />
	<!-- MSM7x27a HAL -->
	<project path="hardware/qcom/display-caf/msm7x27a" name="android_hardware_qcom_display-caf_msm7x27a" revision="cm-14.1" remote="thlg" />
	<project path="hardware/qcom/media-caf/msm7x27a" name="android_hardware_qcom_media-caf_msm7x27a" revision="cm-14.1" remote="thlg" />
	<!-- RIL Legacy -->
	<project path="hardware/ril-legacy" name="android_hardware_ril-legacy" revision="cm-14.1" remote="thlg" />
	<!-- Common Project -->
	<project path="kernel/lge/msm7x27a-common" name="lge-kernel-lproj" revision="android-msm-3.4-mm" remote="thlg" />
	<project path="vendor/lge" name="proprietary_vendor_lge" revision="cm-14.1" remote="thlg" />
	<project path="device/lge/msm7x27a-common" name="android_device_lge_msm7x27a-common" revision="cm-14.1" remote="thlg" />
	<!-- First Generation devices -->
	<project path="device/lge/e610" name="android_device_lge_e610" revision="cm-14.1" remote="thlg" />
	<project path="device/lge/p700" name="android_device_lge_p700" revision="cm-14.1" remote="thlg" />
	<!-- Second Generation devices -->
	<project path="device/lge/v1" name="android_device_lge_v1" revision="cm-14.1" remote="thlg" />
	<project path="device/lge/vee3" name="android_device_lge_vee3" revision="cm-14.1" remote="thlg" />
</manifest>
